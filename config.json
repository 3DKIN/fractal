{
    "title": "Fractal",
    "port": 3000,
    "source": {
        "assets": {
            "dir": "assets",
            "matches": "*"
        },
        "components": {
            "dir": "src/components",
            "matches": "*.+(html|hbs)"
        },
        "views": {
            "dir": "src/views",
            "matches": "*.+(html|hbs)"
        },
        "docs": {
            "dir": "docs",
            "matches": "*.+(html|md|markdown)"
        }
    },
    "theme": {
        "name": "fractal-theme-default"
    },
    "pipes": [
        {
            "name": "frontmatter",
            "priority": 0,
            "matches": "*.+(html|hbs|md|markdown)"
        },
        {
            "name": "markdown",
            "priority": 1,
            "matches": "*.+(md|markdown)"
        }
    ],
    "lang": {
        
    }
    
}