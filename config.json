{
    "title": "Fractal",
    "port": 3000,
    "source": {
        "assets": {
            "dir": "assets"
        },
        "components": {
            "dir": "src/components"
        },
        "views": {
            "dir": "src/views"
        },
        "docs": {
            "dir": "docs"
        }
    },
    "theme": {
        "name": "fractal-theme-default"
    },
    "pipes": [
        {
            "name": "frontmatter",
            "priority": 0,
            "matches": [".hbs",".handlebars",".html",".md",".markdown"]
        },
        {
            "name": "markdown",
            "priority": 1,
            "matches": [".md", ".markdown"]
        }
    ],
    "lang": {
        
    }
    
}