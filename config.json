{
    "title":            "Fractal",
    "port":             3000,
    "assets":           null,
    "components":       null,
    "views":            null,
    "pages":            null,
    "theme":            "fractal-theme-default",
    "sources":          ["assets","components","views","pages"],
    "parsers": [
        {
            "name": "frontmatter",
            "priority": 0,
            "matches": [".hbs",".handlebars",".html",".md",".markdown"]
        },
        {
            "name": "markdown",
            "priority": 1,
            "matches": [".md", ".markdown"]
        }
    ]
    
}